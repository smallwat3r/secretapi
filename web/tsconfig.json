{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "frontend/**/*.ts",
    "frontend/**/*.tsx"
  ]
}
